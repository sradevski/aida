{
  "name": "aida",
  "version": "0.0.1",
  "private": true,
  "description": "Unified data modeling",
  "repository": "https://github.com/sradevski/aida",
  "author": "Stevche Radevski <sradevski@live.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --ci",
    "test:update": "jest -u",
    "test:watch": "jest --watch -u",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "audit": "npm audit",
    "lint": "eslint packages --fix ",
    "precommit": "lint-staged",
    "build": "lerna run build",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna run build && npm run test && lerna publish"
  },
  "jest": {
    "testEnvironment": "node",
    "testURL": "http://localhost:4000",
    "testMatch": [
      "**/?(*.)(unit|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "mjs",
      "node"
    ],
    "collectCoverageFrom": [
      "**/packages/**/*.js",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/output/**",
      "!**/cli/**",
      "!**/__mocks__/**",
      "!**/__tests__/**",
      "!e2e/**"
    ]
  },
  "lint-staged": {
    "packages/**/*.{js,mjs,jsx,css}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-prettier": "^2.6.2",
    "flow-bin": "^0.78.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lerna": "^2.11.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  }
}
